{
  "name": "guifler-driver",
  "version": "1.0.0",
  "description": "Create folder and files",
  "main": "index.js",
  "scripts": {
    "test": "jest --runInBand",
    "test:prod": "npm test -- -c jest.config.prod.js",
    "lint": "eslint --fix . && prettier --write .",
    "start": "node ./dist/src/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec ts-node src/server.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test:integration": "npm run docker:up && npx prisma migrate dev --name init && npm test -- -c jest-config-integration.ts",
    "test:unit": "npm test -- -c jest-config-unit.ts",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma db push",
    "prisma:seed": "ts-node ./prisma/seed.ts",
    "build": "npx tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guiflr/guifler-driver.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/guiflr/guifler-driver/issues"
  },
  "homepage": "https://github.com/guiflr/guifler-driver#readme",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "serverless-http": "^3.2.0",
    "zod": "^3.22.4"
  }
}
